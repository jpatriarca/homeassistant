# Docker Monitoring Integration Configuration
monitor_docker:
  - name: Docker
    url: tcp://192.168.1.11:2375
    containers:
      - hass
      - hydra2
      - ombi
      - radarr
      - jackett
      - grafana
      - swag
      - sonarr
      - radarr
      - bazarr
      - lidarr
      - nzbget
      - netatmo-poller
      - mosquitto
      - qbittorrent
      - unifi-poller
      - oscam
      - telegraf
      - influxdb
      - ouroboros
      - crashplan
      - pihole01-poller
      - pihole02-poller
      - observium
      - tautulli
      - unifi-video
      - unifi-video-mqtt
    rename:
      hass: HomeAssist
      hydra2: Hydra2
      ombi: Ombi
      radarr: Radarr
      jackett: Jackett
      grafana: Grafana
      swag: Swag
      sonarr: Sonarr
      bazarr: Bazarr
      lidarr: Lidarr
      nzbget: NZBget
      netatmo-poller: "Netatmo-Poller"
      mosquitto: Mosquitto
      qbittorrent: qBitTorrent
      unifi-poller: "UniFi-Poller"
      oscam: OScam
      telegraf: Telegraf
      influxdb: InfluxDB
      ouroboros: Ouroboros
      crashplan: CrashPlan
      pihole01-poller: "PiHole01-Poller"
      pihole02-poller: "PiHole02-Poller"
      observium: Observium
      tautulli: Tautulli
      unifi-video: "UniFi Video"
      unifi-video-mqtt: "UniFi Video MQTT"
