binary_sensor:
  - platform: mqtt
    name: Garage Motion
    state_topic: "camera/motion/Garage"
    payload_on: "ON"
    payload_off: "OFF"
    device_class: motion

# Utility Meter configuration

utility_meter:
  hall_plug_daily_energy:
    source: sensor.shelly1pm_68c63afae9ff_power
    cycle: daily
  hall_plug_monthly_energy:
    source: sensor.shelly1pm_68c63afae9ff_power
    cycle: monthly
  hall_plug_yearly_energy:
    source: sensor.shelly1pm_68c63afae9ff_power
    cycle: yearly

